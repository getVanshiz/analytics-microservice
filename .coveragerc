[run]
source = app
omit = 
    */tests/*
    */__pycache__/*

[report]
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if 0:
    if False:
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    
[html]
directory = htmlcov

[xml]
output = coverage.xml